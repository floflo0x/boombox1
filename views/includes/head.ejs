  <% if(data2.length >= 1) { %>
    <meta property="og:title" content="<%= data2[0].title %>" />
    <meta property="twitter:title" content="<%= data2[0].title %>" />
    <% const iwe = data2[0].image.split('.').slice(0, -1).join('.') + '.jpeg'; %>
    <meta property="og:image" content="https://api.hiphopboombox.com/api/uploads/fb/<%= iwe %>" />
    <meta property="twitter:image" content="https://api.hiphopboombox.com/api/uploads/fb/<%= iwe %>" />
    <meta property="og:url" content="" />
    <meta property="twitter:url" content="" />
  <% } else { %>
    <meta property="og:image" content="https://api.hiphopboombox.com/api/uploads/hiphop.jpg" />
    <meta property="twitter:image" content="https://api.hiphopboombox.com/api/uploads/hiphop.jpg" />
  <% } %>
  
